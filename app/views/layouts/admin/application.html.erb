<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title), 'FARMA Administrativo') %></title>

  <%= stylesheet_link_tag    'admin/application', media: 'all', 'data-turbolinks-track' => "reload" %>
  <%= javascript_include_tag 'admin/application', 'data-turbolinks-track' => "reload" %>
  <%= csrf_meta_tags %>
</head>
<body id="<%= controller_name %>-<%= action_name %>">

  <%= render 'shared/flash_messages'  %>

  <div class="navbar-fixed">
    <%= render 'layouts/admin/navbar' %>
  </div>

  <div id="<%= controller_name %>-<%= action_name %>" class="row">

    <div class="col m2 l2">
      <%= render 'layouts/admin/side_navbar' %>
    </div>

    <div class="main-content col m10 l10">
      <div class="content white">
        <%= render_breadcrumbs :builder => ::MaterializeBreadcrumbs,
        class: 'nav-breadcrumbs z-depth-0 transparent'  %>

        <div class="divider"></div>
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
